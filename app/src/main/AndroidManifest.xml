<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.pierry.arctouchcallenge">

  <uses-permission android:name="android.permission.INTERNET"/>

  <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
  <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:name=".ArchApp"
      android:theme="@style/AppTheme">
    <meta-data
        android:name="AA_DB_NAME"
        android:value="archapp11.db"
    />
    <meta-data
        android:name="AA_DB_VERSION"
        android:value="1"
    />
    <meta-data
        android:name="AA_MODELS"
        android:value="com.github.pierry.arctouchcallenge.domain.Stop,
        com.github.pierry.arctouchcallenge.domain.Departure"
    />


    <activity android:name=".MainActivity_">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".DetailsActivity_"/>
    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="@string/google_maps_key"/>

  </application>

</manifest>